{"ast":null,"code":"import { mapState } from \"vuex\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      username: \"djsmanoj0000\",\n      projects: []\n    };\n  },\n  methods: {\n    get_leet_info() {\n      this.username == \"djsmanoj0000\";\n      axios.get(\"https://api.github.com/users/ManojDjs/repos\").then(resp => {\n        console.log(resp.data);\n        this.projects = resp.data.sort(this.custom_sort).slice(-3).reverse();\n        console.log(this.projects);\n      });\n    },\n    custom_sort(a, b) {\n      return new Date(a.created_at).getTime() - new Date(b.created_at).getTime();\n    },\n    open_git(url) {\n      window.open(url);\n    },\n    get_random_color() {\n      return {\n        backgroundColor: \"#00000\"\n      };\n    }\n  },\n  created() {\n    this.get_leet_info();\n  },\n  computed: {\n    ...mapState([\"leetcode\"])\n  }\n};","map":{"version":3,"mappings":"AA+GA,SAASA,QAAO,QAAS,MAAM;AAC/B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,cAAc;MACxBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAa,GAAG;MACd,IAAI,CAACH,QAAO,IAAK,cAAc;MAC/BF,KAAK,CAACM,GAAG,CAAC,6CAA6C,CAAC,CAACC,IAAI,CAAEC,IAAI,IAAK;QACtEC,OAAO,CAACC,GAAG,CAACF,IAAI,CAACP,IAAI,CAAC;QACtB,IAAI,CAACE,QAAO,GAAIK,IAAI,CAACP,IAAI,CAACU,IAAI,CAAC,IAAI,CAACC,WAAW,CAAC,CAACC,KAAK,CAAC,CAAC,CAAC,CAAC,CAACC,OAAO,EAAE;QACpEL,OAAO,CAACC,GAAG,CAAC,IAAI,CAACP,QAAQ,CAAC;MAC5B,CAAC,CAAC;IACJ,CAAC;IACDS,WAAW,CAACG,CAAC,EAAEC,CAAC,EAAE;MAChB,OACE,IAAIC,IAAI,CAACF,CAAC,CAACG,UAAU,CAAC,CAACC,OAAO,EAAC,GAAI,IAAIF,IAAI,CAACD,CAAC,CAACE,UAAU,CAAC,CAACC,OAAO,EAAC;IAEtE,CAAC;IACDC,QAAQ,CAACC,GAAG,EAAE;MACZC,MAAM,CAACC,IAAI,CAACF,GAAG,CAAC;IAClB,CAAC;IACDG,gBAAgB,GAAG;MACjB,OAAO;QAAEC,eAAe,EAAE;MAAS,CAAC;IACtC;EACF,CAAC;EACDC,OAAO,GAAG;IACR,IAAI,CAACrB,aAAa,EAAE;EACtB,CAAC;EACDsB,QAAQ,EAAE;IACR,GAAG5B,QAAQ,CAAC,CAAC,UAAU,CAAC;EAC1B;AACF,CAAC","names":["mapState","axios","data","username","projects","methods","get_leet_info","get","then","resp","console","log","sort","custom_sort","slice","reverse","a","b","Date","created_at","getTime","open_git","url","window","open","get_random_color","backgroundColor","created","computed"],"sourceRoot":"","sources":["D:\\vue_codes\\MY_CV\\cv\\src\\components\\Aboutme.vue"],"sourcesContent":["<template>\r\n  <div class=\"card\">\r\n    <div\r\n      class=\"\r\n        grid grid-nogutter\r\n        flex-wrap\r\n        p-2\r\n        lg:p-4\r\n        bg-cyan-50\r\n        border-round\r\n        mb-4\r\n        text-center\r\n        lg:text-left\r\n      \"\r\n    >\r\n      <div class=\"px-4 py-8 md:px-6 lg:px-8\">\r\n        <div class=\"font-bold text-5xl text-indigo-900 mb-3\">\r\n          Recent Projects\r\n        </div>\r\n        <div class=\"text-indigo-700 line-height-3 text-xl mb-6\">\r\n          I am working on this projects currently, feel free to coloborate. I am\r\n          always open to learn new technologies.\r\n        </div>\r\n\r\n        <div\r\n          class=\"\r\n            flex flex-column\r\n            align-items-center\r\n            justify-content-center\r\n            lg:flex-row\r\n            gap-3\r\n          \"\r\n        >\r\n          <div v-bind:key=\"item\" v-for=\"item in projects\">\r\n            <div\r\n              class=\"shadow-2 border-round-xl hover:shadow-8\"\r\n              style=\"\r\n                background: linear-gradient(\r\n                  15deg,\r\n                  rgb(249, 22, 22),\r\n                  rgb(249, 115, 22)\r\n                );\r\n                border-left: 10px solid;\r\n              \"\r\n            >\r\n              <div class=\"p-4 border-round card h-max-5 h-min-5\">\r\n                <div class=\"flex mb-3\">\r\n                  <div\r\n                    class=\"p-avatar p-component p-avatar-image p-avatar-circle\"\r\n                  >\r\n                    <img\r\n                      src=\"https://avatars.githubusercontent.com/u/22208973?v=4\"\r\n                    />\r\n                  </div>\r\n\r\n                  <div class=\"ml-2\">\r\n                    <div class=\"font-bold text-indigo-900 mb-1\">\r\n                      Owner {{ item.owner.login }}\r\n                    </div>\r\n                    <div\r\n                      class=\"text-sm flex align-items-center text-indigo-700\"\r\n                    >\r\n                      <span> last updated at </span>\r\n                      <div\r\n                        class=\"border-circle surface-300 mx-2\"\r\n                        style=\"width: 0.25rem; height: 0.25rem\"\r\n                      ></div>\r\n                      <span>{{ item.updated_at }}</span>\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n                <div\r\n                  class=\"text-xl p-3 mb-3 font-medium mb-3 line-height-3\"\r\n                  style=\"border-left: 8px solid\"\r\n                >\r\n                  <span class=\"text-indigo-900\">{{ item.name }}</span>\r\n                </div>\r\n                <div\r\n                  class=\"\r\n                    line-height-3\r\n                    text-indigo-700\r\n                    overflow-hidden\r\n                    text-overflow-ellipsis\r\n                  \"\r\n                >\r\n                  Description: {{ item.description }}\r\n                </div>\r\n                <span class=\"text-indigo-900 font-bold p-component mr-2\"\r\n                  ><!----><span class=\"mr-2\">\r\n                    Written in: {{ item.language }}</span\r\n                  ></span\r\n                >\r\n                <div class=\"template mt-2\">\r\n                  <Button\r\n                    class=\"google p-0\"\r\n                    aria-label=\"Google\"\r\n                    @click=\"open_git(item.html_url)\"\r\n                  >\r\n                    <i class=\"pi pi-github px-2\"></i>\r\n                    <span class=\"px-3\">Github</span>\r\n                  </Button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n    <script>\r\nimport { mapState } from \"vuex\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: \"djsmanoj0000\",\r\n      projects: [],\r\n    };\r\n  },\r\n  methods: {\r\n    get_leet_info() {\r\n      this.username == \"djsmanoj0000\";\r\n      axios.get(\"https://api.github.com/users/ManojDjs/repos\").then((resp) => {\r\n        console.log(resp.data);\r\n        this.projects = resp.data.sort(this.custom_sort).slice(-3).reverse();\r\n        console.log(this.projects);\r\n      });\r\n    },\r\n    custom_sort(a, b) {\r\n      return (\r\n        new Date(a.created_at).getTime() - new Date(b.created_at).getTime()\r\n      );\r\n    },\r\n    open_git(url) {\r\n      window.open(url);\r\n    },\r\n    get_random_color() {\r\n      return { backgroundColor: \"#00000\" };\r\n    },\r\n  },\r\n  created() {\r\n    this.get_leet_info();\r\n  },\r\n  computed: {\r\n    ...mapState([\"leetcode\"]),\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n.p-button {\r\n  margin-right: 0.5rem;\r\n}\r\n\r\n.p-buttonset {\r\n  .p-button {\r\n    margin-right: 0;\r\n  }\r\n}\r\n\r\n.sizes {\r\n  .button {\r\n    margin-bottom: 0.5rem;\r\n    display: block;\r\n\r\n    &:last-child {\r\n      margin-bottom: 0;\r\n    }\r\n  }\r\n}\r\n\r\n.template .p-button i {\r\n  line-height: 2.25rem;\r\n}\r\n.template .p-button.google {\r\n  background: linear-gradient(\r\n    to left,\r\n    var(--bluegray-700) 50%,\r\n    var(--bluegray-800) 50%\r\n  );\r\n  background-size: 200% 100%;\r\n  background-position: right bottom;\r\n  transition: background-position 0.5s ease-out;\r\n  color: #fff;\r\n  border-color: var(--bluegray-900);\r\n}\r\n.template .p-button.google:hover {\r\n  background-position: left bottom;\r\n}\r\n.template .p-button.google i {\r\n  background-color: var(--bluegray-900);\r\n}\r\n.template .p-button.google:focus {\r\n  box-shadow: 0 0 0 1px var(--bluegray-900);\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}